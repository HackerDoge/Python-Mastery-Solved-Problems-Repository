   "source": [
    "def Maxi(A):\n",
    "    A.sort()\n",
    "    N=len(A)\n",
    "    P1=A[N-1]*A[0]*A[1]\n",
    "    P2=A[N-1]*A[N-2]*A[N-3]\n",
    "    return max(P1,P2)\n",
    "    pass\n",
    "\n",
    "A=[-3,1,2,-2,5,6]\n",
    "Maxi(A)"
   ]
