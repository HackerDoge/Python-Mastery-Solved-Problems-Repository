   "source": [
    "def reverser(string):\n",
    "    rev=string.split()[::-1]\n",
    "    return \" \".join(rev)\n",
    "    \n",
    "string=(input(\"Enter the String: \"))\n",
    "res= reverser(string)\n",
    "print(\"Reversed String is: \",res)"
   ]
